alias sc="source ${TOOLS}/setclone.sh "
alias generate-custom-alias="${TOOLS}/environment/write_tools_alias.sh"
alias index="python ${TOOLS}/generatetags.py"

#Gerrit-tools
alias gerrit-query="${TOOLS}/gerrit-tools/gerrit-query.py"
